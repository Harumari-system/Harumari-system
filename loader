GameText.Text = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local function ASYB_fake_script()
	while LoadingStage ~= nil  do
	if LoadingStage == "Preload" then
		Loading.Text = "Loading."
		wait(.5)
		Loading.Text = "Loading.."
		wait(.5)
		Loading.Text = "Loading..."
		wait(.5)
	elseif LoadingStage == "Key" then
	Loading.Text = "Checking Key."
		wait(.5)
		Loading.Text = "Checking Key.."
		wait(.5)
		Loading.Text = "Checking Key..."
		wait(.5)
	else 
	break
	end
	end
end
coroutine.wrap(ASYB_fake_script)()

wait(3)
game:GetService("TweenService"):Create(Loading,TweenInfo.new(1),{TextTransparency = 1}):Play()
wait(1)
LoadingStage = "Key"
wait(1)
game:GetService("TweenService"):Create(Loading,TweenInfo.new(1),{TextTransparency = 0}):Play()
wait(2)
if not success then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/NotMinhDucGamingTV/Harumari-system/main/key'))()
	game:GetService("TweenService"):Create(Loading,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(ImageLabel,TweenInfo.new(1),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(GameText,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(Credit,TweenInfo.new(1),{TextTransparency = 1}):Play()
	wait(.5)
	local CloseTween = game:GetService("TweenService"):Create(Placeholder,TweenInfo.new(1),{Size = UDim2.new(0, 518, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)})
	CloseTween:Play()
	CloseTween.Completed:Connect(function()
		Loader:Destroy()
	end)
	return
	else
	local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
		local res = nil 
		res = game:HttpGet("https://harumari-system.notminhducgamingtv.tk/api/checkkey?key="..KeyContent.."&hwid="..hwid)
		while res == nil do
		 print("Waiting for response from webserver")
		end
		if res == "authenticated" then
			_G.Type = "FREE"
			local script = loadstring(game:HttpGet('https://raw.githubusercontent.com/NotMinhDucGamingTV/Harumari-system/main/Script/'..tostring(game.PlaceId)..'.lua'))
			if script then
				script()
			else
				LoadingStage = "Failed"
				wait(1)
				Loading.Text = "This game is not supported."
			end
	game:GetService("TweenService"):Create(Loading,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(ImageLabel,TweenInfo.new(1),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(GameText,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(Credit,TweenInfo.new(1),{TextTransparency = 1}):Play()
	wait(.5)
	local CloseTween = game:GetService("TweenService"):Create(Placeholder,TweenInfo.new(1),{Size = UDim2.new(0, 518, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)})
	CloseTween:Play()
	CloseTween.Completed:Connect(function()
		Loader:Destroy()
	end)
		elseif res == "authenticated-premium" then
		_G.Type = "PREMIUM"
		local script = loadstring(game:HttpGet('https://raw.githubusercontent.com/NotMinhDucGamingTV/Harumari-system/main/Script/'..tostring(game.PlaceId)..'-premium.lua'))
			if script then
				script()
			else
				LoadingStage = "Failed"
				wait(1)
				Loading.Text = "This game is not supported."
			end
	game:GetService("TweenService"):Create(Loading,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(ImageLabel,TweenInfo.new(1),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(GameText,TweenInfo.new(1),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(Credit,TweenInfo.new(1),{TextTransparency = 1}):Play()
	wait(.5)
	local CloseTween = game:GetService("TweenService"):Create(Placeholder,TweenInfo.new(1),{Size = UDim2.new(0, 518, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)})
	CloseTween:Play()
	CloseTween.Completed:Connect(function()
		Loader:Destroy()
	end)
		end
end
end)
